{
  "manifest_version": 2,
  "default_locale":"zh",
  "name": "bjut-vpn-connector",
  "description": "整合北京工业大学官方提供的的VPN和WebVpn，模拟校园网环境访问内网的体验。",
  "version": "1.2",
  "browser_action": {
    "default_icon": "./img/icon128.png",
    "default_popup": "popup.html",
    "default_title": "点击连接到北京工业大学VPN"
  },
  "icons": {
    "128": "./img/icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": false
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "*://*.bjut.edu.cn/*",
    "notifications"
  ],
  "optional_permissions": [
    "contextMenus"
  ],
  "homepage_url": "https://github.com/Woodykaixa/bjut-vpn-connector"
}