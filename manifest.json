{
  "manifest_version": 2,
  "name": "bjut-vpn-connector",
  "description": "Help you connecting vpn and WebVpn of Beijing University of Technology",
  "version": "0.7.2",
  "browser_action": {
    "default_icon": "./img/VPN.png",
    "default_popup": "popup.html",
    "default_title": "点击连接到北京工业大学VPN"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "*://*.bjut.edu.cn/*",
    "notifications"
  ]
}