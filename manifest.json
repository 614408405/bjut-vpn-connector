{
  "manifest_version": 2,
  "name": "bjut-vpn-connector",
  "description": "Help you connecting vpn and WebVpn of Beijing University of Technology",
  "version": "0.8.1",
  "browser_action": {
    "default_icon": "./img/VPN.png",
    "default_popup": "popup.html",
    "default_title": "点击连接到北京工业大学VPN"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": false
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "*://*.bjut.edu.cn/*",
    "notifications"
  ],
  "homepage_url": "https://github.com/Woodykaixa/bjut-vpn-connector"
}