{
  "manifest_version": 2,
  "name": "bjut-vpn-connector",
  "description": "Connect bjut vpn",
  "version": "0.6.5",
  "browser_action": {
    "default_icon": "./img/VPN.png",
    "default_popup": "popup.html",
    "default_title": "点击连接到北京工业大学VPN"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options.html",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "*://*.bjut.edu.cn/*"
  ]
}